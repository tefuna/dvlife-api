<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jp.tefuna.dvlife.infrastructure.db.mapper.ExchangeRateMapper">

  <!--
  <resultMap id="ExchangeRate" type="ExchangeRate">
    <const>

  </resultMap>
-->
  <select id="findByRateId" resultType="ExchangeRate">
    SELECT
      rate_id,
      base_date,
      currency,
      rate_origin,
      rate
    FROM
      dvl.exchange_rates
    WHERE
      rate_id = #{rateId.rateId}
  </select>

</mapper>
